    "db:up:bun": "bun node_modules/node-pg-migrate/bin/node-pg-migrate -m src/config/pg/migrations up",
    "db:down:bun": "bun node_modules/node-pg-migrate/bin/node-pg-migrate -m src/config/pg/migrations down",
