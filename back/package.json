    "test:e2e": "jest --watchAll --testRegex=.e2e.test.ts",
    "test:integration": "jest --runInBand --testRegex=.integration.test.ts",
    "test:manual": "jest --runInBand --watchAll --testRegex=.manual.test.ts",
    "test": "jest --testRegex='^((?!(integration|manual)).)*.test.ts$'",
    "test:all": "jest --runInBand --testRegex='^((?!manual).)*.test.ts$'",
    "format": "pnpm -w prettier:glob --write ./back/src",
    "format:check": "pnpm -w prettier:glob --check ./back/src",
    "typecheck": "tsc --noEmit",
    "includecheck": "./check_includes.sh",
    "fullcheck": "npm run includecheck && npm run format && npm run lint && npm run test && npm run typecheck",
    "fullcheck-with-integration": "npm run format && npm run test:all && npm run typecheck",
    "migrate": "ts-node node_modules/node-pg-migrate/bin/node-pg-migrate -j ts -m src/adapters/secondary/pg/migrations",
    "lint": "pnpm -w lintconfig:glob ./back/src"
