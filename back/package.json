    "test:integration": "jest --runInBand --watchAll --testRegex=.integration.test.ts",
    "test:manual": "jest --runInBand --watchAll --testRegex=.manual.test.ts",
    "test": "jest --runInBand --testRegex='^((?!(integration|manual)).)*.test.ts$'",
    "test:all": "jest --runInBand --testRegex='^((?!manual).)*.test.ts$'",
