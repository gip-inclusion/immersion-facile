{
  "version": "0.0.0",
  "scripts": {
    "test": "jest --passWithNoTests --verbose --testRegex='^((?!(integration|manual)).)*.test.ts$'",
    "test:manual": "jest --runInBand --watchAll --testRegex=.manual.test.ts",
    "dev": "VITE_GATEWAY=IN_MEMORY VITE_ENV_TYPE=DEV VITE_PREFILLED_FORMS=true vite",
    "dev-http": "VITE_ENV_TYPE=DEV VITE_PREFILLED_FORMS=true vite",
    "build": "vite build",
    "build-with-shared-as-is": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "includecheck": "./check_includes.sh",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "fullcheck": "npm run includecheck && npm run format && npm run test && npm run typecheck && npm run lint",
    "lint": "pnpm -w run lint:front"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^1.0.5",
