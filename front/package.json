    "@reduxjs/toolkit": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.2",
    "formik": "2.2.9",
    "prop-types": "^15.8.1",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-design-system": "workspace:*",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.5.5",
    "shared": "workspace:*",
    "type-route": "^0.6.0",
    "uuid": "^8.3.2",
    "zod": "^3.14.2"
