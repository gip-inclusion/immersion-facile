    "@types/node": "^17.0.31",
    "@types/prop-types": "^15.7.5",
    "@types/ramda": "^0.28.1",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.23",
