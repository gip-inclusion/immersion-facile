{
  "version": "0.0.0",
  "scripts": {
    "test": "npm run symlink-or-copy-shared && jest --passWithNoTests --verbose",
    "dev": "npm run symlink-shared && VITE_GATEWAY=IN_MEMORY VITE_ENV_TYPE=DEV VITE_PREFILLED_ESTABLISHMENT_FORM=true vite",
    "dev-http": "npm run symlink-shared && VITE_ENV_TYPE=DEV vite",
    "build": "npm run symlink-or-copy-shared && vite build",
    "build-with-shared-as-is": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "includecheck": "./check_includes.sh",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "fullcheck": "npm run includecheck && npm run format && npm run test && npm run typecheck && npm run lint",
    "symlink-or-copy-shared": "if [ ! -z $CI ]; then npm run copy-shared; else npm run symlink-shared; fi;",
    "copy-shared": "rm -rf ./src/shared; cp -Rf ../back/src/shared ./src",
    "symlink-shared": "rm -rf ./src/shared; ln -fs ../../back/src/shared ./src/;",
    "lint": "eslint ./src --fix"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^1.0.5",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@gouvfr/dsfr": "1.1.0",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "formik": "2.2.9",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.5.5",
    "type-route": "^0.6.0",
    "uuid": "^8.3.2",
    "zod": "^3.14.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/ramda": "^0.28.1",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  }
}
