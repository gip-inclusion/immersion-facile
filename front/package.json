{
  "name": "front",
  "version": "0.0.0",
  "scripts": {
    "test": "jest --verbose --testRegex='^((?!(integration|manual)).)*.test.ts$'",
    "test:manual": "jest --runInBand --watchAll --testRegex=.manual.test.ts",
    "setup-env-vars": "chmod +x ./env.sh && ./env.sh",
    "setup-env-vars-build": "chmod +x ./env.sh && ./env.sh ./dist/env-config.js",
    "dev-in-memory": "VITE_GATEWAY=IN_MEMORY pnpm setup-env-vars && vite",
    "dev": "export $(cat .env | grep 'VITE_' | xargs) && pnpm setup-env-vars && vite",
    "dev:no-typecheck": "export $(cat .env | grep 'VITE_' | xargs) && pnpm setup-env-vars && vite",
    "make-styles": "scss-to-ts update",
    "build-scalingo": "rm -rf scalingo/dist && vite build && mv dist scalingo/dist && cp env.sh scalingo/dist && tar -czf front-build.tar.gz scalingo",
    "build": "vite build && export $(cat .env | grep 'VITE_' | xargs) && pnpm setup-env-vars-build",
    "serve": "vite preview --port 5001",
    "typecheck": "tsc --noEmit",
    "includecheck": "./check_includes.sh",
    "check": "pnpm -w check:glob ./front/src",
    "fullcheck": "pnpm -w fullcheck --filter=front",
    "predev": "react-dsfr update-icons",
    "prebuild": "react-dsfr update-icons"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@codegouvfr/react-dsfr": "^1.13.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@reduxjs/toolkit": "^1.8.1",
    "@sentry/browser": "^7.119.1",
    "@sentry/react": "^9.6.0",
    "@sentry/vite-plugin": "^3.3.1",
    "@types/jest": "^29.5.2",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.17.16",
    "@types/papaparse": "^5.3.7",
    "@types/ramda": "^0.30.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-lines-ellipsis": "^0.15.1",
    "@types/react-redux": "^7.1.23",
    "@types/swagger-ui-react": "^4.18.0",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.4",
    "axios": "^1.8.2",
    "babel-jest": "^29.5.0",
    "date-fns": "2.28.0",
    "html-templates": "workspace:*",
    "jest": "^29.5.0",
    "leaflet": "^1.9.4",
    "openapi-types": "^12.1.3",
    "papaparse": "^5.3.2",
    "postcss": "^8.4.31",
    "ramda": "^0.30.1",
    "react": "^18.0.0",
    "react-design-system": "workspace:*",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.42.1",
    "react-leaflet": "^4.2.1",
    "react-redux": "^7.2.8",
    "react-select": "^5.9.0",
    "redux": "^4.2.0",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.5.5",
    "sass": "^1.55.0",
    "scss-mapper": "workspace:*",
    "shared": "workspace:*",
    "shared-routes": "^0.8.31",
    "swagger-ui-react": "^5.1.0",
    "ts-pattern": "^5.7.0",
    "tss-react": "^4.5.2",
    "type-route": "^1.0.0",
    "typescript": "^5.7.3",
    "uuid": "^8.3.2",
    "vite": "^5.4.19",
    "vite-plugin-pwa": "^0.17.2",
    "workbox-window": "^7.0.0",
    "zod": "3.22.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1"
  }
}
